{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "None"
      }
    }
  },
  "AuthProviders": {
    "AzureAD": {
      "Authority": "https://login.microsoftonline.com",
      "ClientId": "",
      "ClientSecret": "",
      "SignedOutCallbackPath": "/account/signed-out",
      "CallbackPath": "/account/sign-in/openid",
      "Scope": [ "openid", "profile", "email", "roles", "sub", "tid", "family_name", "given_name", "idp" ]
    },
    "Google": {
      "ClientId": "",
      "ClientSecret": "",
      "CallbackPath": "/account/sign-in/openid",
      "SignedOutCallbackPath": "/account/signed-out",
      "Scope": [
        "openid",
        "https://www.googleapis.com/auth/plus.me",
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/userinfo.profile" 
      ]
    }
  },
  "OpenId": {
    "ClientId": "",
    "SignedOutCallbackPath": "/account/signed-out",
    "SigningCert": {
      "Thumbprint": "",
      "Password": ""
    }
  },
  "ConnectionStrings": {
    "Identity": "",
    "TenantDirectory": ""
  }
}
